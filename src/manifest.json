{
    "name": "Barebone Extension",
    "description": "Framework for building extension",
    "version": "1.0.0",
    "icons": {
        "128": "public/img/Common/planets.png"
    },
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Barebone Extension",
        "default_popup": "public/html/Common/auth.html"
    },
    "permissions": ["activeTab", "tabs", "notifications", "webRequest", "*://*/*", "*://www.google.com/*", "webNavigation", "history", "unlimitedStorage"],
    "web_accessible_resources": ["/public/**", "/Vendor/**"],
    "background": {
        "scripts": ["Background.js"]
    }
}
